<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="false" version="3.0">
  <welcome-file-list>
    <welcome-file>formularz.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MovieListServlet</servlet-name>
    <servlet-class>movie.MovieListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddNewMovieServlet</servlet-name>
    <servlet-class>movie.AddNewMovieServlet</servlet-class>
  </servlet>
  

  <servlet-mapping>
    <servlet-name>MovieListServlet</servlet-name>
    <url-pattern>/lista.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddNewMovieServlet</servlet-name>
    <url-pattern>/newMovie</url-pattern>
  </servlet-mapping>
  
 
</web-app>